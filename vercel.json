{
  "version": 2,
  "_comment": "Updated to force Vercel to use latest commit",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://defidojo-backend.onrender.com",
    "NEXT_PUBLIC_WS_URL": "wss://defidojo-backend.onrender.com"
  }
}
